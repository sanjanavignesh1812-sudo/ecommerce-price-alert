    {
      "parameters": {
        "jsCode": "const rows = items.map(i => i.json);\n\n// sort by fetched_at (oldest â†’ newest)\nrows.sort((a, b) => new Date(a.fetched_at) - new Date(b.fetched_at));\n\n// if only one record exists, no comparison\nif (rows.length < 2) {\n  return [{\n    json: {\n      ...rows[rows.length - 1],\n      price_dropped: false,\n      previous_price: null,\n      current_price: rows[rows.length - 1].price\n    }\n  }];\n}\n\nconst previous = rows[rows.length - 2];\nconst current = rows[rows.length - 1];\n\nreturn [{\n  json: {\n    id: current.id,\n    title: current.title,\n    previous_price: Number(previous.price),\n    current_price: Number(current.price),\n    price_dropped: Number(current.price) < Number(previous.price),\n    fetched_at: current.fetched_at\n  }\n}];\n"
      },
